{
  "buildCommand": "corepack enable && pnpm install && pnpm run build",
  "devCommand": "pnpm dev",
  "installCommand": "corepack enable && pnpm install",
  "framework": "nextjs",
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "1"
  },
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 10
    }
  },
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
